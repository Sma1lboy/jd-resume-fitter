  "scripts": {
    "dev": "npm run dev:chrome",
    "dev:chrome": "node copy-pdf-worker.js && cross-env NODE_ENV=development TARGET_BROWSER=chrome webpack -w --config webpack.config.js",
    "dev:firefox": "node copy-pdf-worker.js && cross-env NODE_ENV=development TARGET_BROWSER=firefox webpack -w --config webpack.config.js",
    "dev:opera": "node copy-pdf-worker.js && cross-env NODE_ENV=development TARGET_BROWSER=opera webpack -w --config webpack.config.js",
    "dev:edge": "node copy-pdf-worker.js && cross-env NODE_ENV=development TARGET_BROWSER=edge webpack -w --config webpack.config.js",
    "build": "npm run build:chrome",
    "build:chrome": "cross-env NODE_ENV=production TARGET_BROWSER=chrome webpack --config webpack.config.js && node copy-pdf-worker.js",
    "build:firefox": "cross-env NODE_ENV=production TARGET_BROWSER=firefox webpack --config webpack.config.js && node copy-pdf-worker.js",
    "build:opera": "cross-env NODE_ENV=production TARGET_BROWSER=opera webpack --config webpack.config.js && node copy-pdf-worker.js",
    "build:edge": "cross-env NODE_ENV=production TARGET_BROWSER=edge webpack --config webpack.config.js && node copy-pdf-worker.js",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix"
  },
